    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define-from-file",
                ".env"
            ]
        },
        {
            "name": "web: debug",
            "request": "launch",
            "type": "dart",
            "args": [
                "-d",
                "edge",
                "--web-browser-flag",
                "--disable-web-security",
                "--dart-define-from-file",
                ".env"
            ]
        }
    ]
}